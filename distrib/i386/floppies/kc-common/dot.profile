#	$OpenBSD: src/distrib/i386/floppies/kc-common/Attic/dot.profile,v 1.2 1996/04/25 21:28:17 niklas Exp $

PATH=/sbin:/bin:/
export PATH

if [ "X${DONEPROFILE}" = "X" ]; then
	DONEPROFILE=YES

	echo -n	"Enter 'copy_kernel' at the prompt to copy a kernel to your "
	echo	"hard disk,"
	echo	"'reboot' to reboot the system, or 'halt' to halt the system."
	echo	""
fi
