	sh ${.CURDIR}/testenc.sh ${.OBJDIR} ${.CURDIR}
	sh ${.CURDIR}/testdsa.sh ${.OBJDIR} ${.CURDIR}
	sh ${.CURDIR}/testrsa.sh ${.OBJDIR} ${.CURDIR}

.include <bsd.prog.mk>
