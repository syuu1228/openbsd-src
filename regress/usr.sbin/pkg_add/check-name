use Test::Simple tests => 1;
use OpenBSD::PkgSpec;

my @list=qw(py-MxDateTime-2.0.1-py2.1);

ok(OpenBSD::PkgSpec::match('py-MxDateTime->=2.0-py2.1', @list) == 1);
