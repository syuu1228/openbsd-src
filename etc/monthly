#!/bin/sh -
#	$OpenBSD: src/etc/monthly,v 1.4 1997/09/15 09:54:40 deraadt Exp $

host=`hostname -s`
echo "Subject: $host monthly run output"

if [ -f /etc/monthly.local ];then
	echo ""
	echo "Running monthly.local:"
	. /etc/monthly.local
else
	echo "Nothing to do!"
fi

